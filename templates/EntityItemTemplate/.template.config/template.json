{
  "$schema": "http://json.schemastore.org/template",
  "author": "王建军",
  "classifications": [ "Common", "Code" ],
  "identity": "Arctic.EntityItemTemplate",
  "name": "Arctic.EntityItemTemplate",
  "shortName": "arctic-entity",
  "tags": {
    "language": "C#",
    "type": "item"
  },
  "preferNameDirectory":true,
  "symbols":{
    "EntityName": {
      "type": "parameter",
      "defaultValue": "Clazz",
      "replaces":"Clazz",
      "isRequired":true,
      "fileRename": "Clazz"
    },  
    "Namespace": {
      "type": "parameter",
      "defaultValue": "Application",
      "replaces":"Application"
    }    
  }
}